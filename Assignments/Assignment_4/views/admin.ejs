.admin-container {
  background-color: #f9f9f9;
  color: #814b72;
}
.admin-header {
  background-color: #814b72;
  color: white;
  padding: 1rem;
  box-shadow: 0 2px 10px rgba(129, 75, 114, 0.2);
}
.admin-sidebar {
  background-color: #814b72;
  color: white;
}
.admin-sidebar .nav-link {
  color: rgba(255, 255, 255, 0.8);
  transition: all 0.3s ease;
}
.admin-sidebar .nav-link:hover {
  background-color: #9a5b8a;
  color: white;
}
.admin-sidebar .nav-link.active {
  background-color: #9a5b8a;
  color: white;
}
.btn-primary {
  background-color: #814b72;
  border-color: #814b72;
}
.btn-primary:hover {
  background-color: #9a5b8a;
  border-color: #9a5b8a;
}
.table thead {
  background-color: #814b72;
  color: white;
}
.table-hover tbody tr:hover {
  background-color: rgba(129, 75, 114, 0.1);
}
.card {
  border-color: #814b72;
}
.card-header {
  background-color: #814b72;
  color: white;
}
.form-control:focus {
  border-color: #814b72;
  box-shadow: 0 0 0 0.25rem rgba(129, 75, 114, 0.25);
}
.pagination .page-item.active .page-link {
  background-color: #814b72;
  border-color: #814b72;
}
.pagination .page-link {
  color: #814b72;
}
.pagination .page-link:hover {
  color: #9a5b8a;
}
.badge-primary {
  background-color: #814b72;
}
.alert-primary {
  background-color: rgba(129, 75, 114, 0.1);
  border-color: #814b72;
  color: #814b72;
}
.text-primary {
  color: #814b72 !important;
}
.border-primary {
  border-color: #814b72 !important;
} 